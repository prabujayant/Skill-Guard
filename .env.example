# Environment configuration
# Copy this file to .env and fill in your values

# LLM API Keys (required for LLM-based audit)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# GitHub Token (for scraping - optional but recommended for higher rate limits)
GITHUB_TOKEN=

# Database (optional, defaults to SQLite)
# DATABASE_URL=postgresql://user:pass@localhost/skillguard

# Logging
SKILLGUARD_LOG_LEVEL=INFO

# Analysis settings
SKILLGUARD_MAX_WORKERS=4
SKILLGUARD_BATCH_SIZE=100
